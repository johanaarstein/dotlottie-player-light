{
  "jsc": {
    "baseUrl": ".",
    "assumptions": {
      "setPublicClassFields": true
    },
    "keepClassNames": true,
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "decoratorsBeforeExport": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "minify": {
      "compress": false,
      "mangle": false
    }
  },
  "minify": true
}